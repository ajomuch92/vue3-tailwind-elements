{
  "name": "vue3-tailwind-elements",
  "license": "MIT",
  "homepage": "https://vue-tailwind-elements.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajomuch92/vue3-tailwind-elements.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly && ncp ./src/plugin ./dist/plugin",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "postcss": "^8.4.21",
    "rollup-plugin-typescript2": "^0.34.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vue-tsc": "^1.0.24"
  },
  "private": false,
  "version": "0.0.3-beta",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-tailwind-elements.umd.js",
  "module": "./dist/vue3-tailwind-elements.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-tailwind-elements.es.js",
      "require": "./dist/vue3-tailwind-elements.umd.js"
    },
    "./dist/vue3-tailwind-elements.css": {
      "import": "./dist/vue3-tailwind-elements.css",
      "require": "./dist/vue3-tailwind-elements.css"
    }
  },
  "types": "./dist/main.d.ts"
}